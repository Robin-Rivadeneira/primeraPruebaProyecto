{
  "expo": {
    "name": "firmaelectronica",
    "slug": "firmaelectronica",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/goIdentity.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/img/goIdentity.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.robinjos.firmaelectronica",
      "infoPlist": {
        "NSCameraUsageDescription": "Esta aplicación necesita acceso a la cámara para escanear códigos QR."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/goIdentity.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.robinjos.firmaelectronica",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO"
      ],
      "versionCode": 1,
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/img/goIdentity.png"
    },
    "owner": "robinjos",
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Permiso para usar la cámara",
          "microphonePermission": "Permiso para usar el micrófono"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "0b1e5ea3-a32e-47c1-ba65-a99d3c7296d1"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}